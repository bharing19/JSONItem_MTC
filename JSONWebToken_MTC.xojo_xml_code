  <ItemSource>
   <TextEncoding>134217984</TextEncoding>
   <SourceLine>Shared Function Validate(token As String, secret As String) As JSONWebToken_MTC</SourceLine>
   <SourceLine>dim result as new JSONWebToken_MTC</SourceLine>
